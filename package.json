    "axios": "^1.7.4",
    "dayjs": "^1.11.13",
    "formik": "^2.4.6",
    "i18next": "^23.14.0",
    "json-server": "^1.0.0-beta.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.53.0",
    "react-i18next": "^15.0.1",
